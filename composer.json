{
    "name": "exs/bulk-email-checker-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides an email validation through 'Bulk Email Checker' api.",
    "keywords": ["Symfony2", "bundle", "email", "validation"],
    "homepage": "https://github.com/ExSituMarketing/EXS-BulkEmailCheckerBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ex-Situ Marketing",
            "email": "dev@ex-situ.com"
        }
    ],
    "require": {
        "php": "~5.4|~7.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/validator": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.9.2"
    },
    "autoload": {
        "psr-4": { "EXS\\BulkEmailCheckerBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
